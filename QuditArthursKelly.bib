@article{monomial,
author = {Appleby, D. M. and Bengtsson, Ingemar and Brierley, Stephen and Grassl, Markus and Gross, David and Larsson, Jan-\r{A}ke},
title = {{The monomial representations of the Clifford group}},
year = {2012},
issue_date = {May 2012},
publisher = {Rinton Press, Incorporated},
address = {Paramus, NJ},
volume = {12},
number = {5–6},
issn = {1533-7146},
abstract = {We show that the Clifford group--the normaliser of the Weyl-Heisenberg group--can be represented by monomial phase-permutation matrices if and only if the dimension is a square number. This simplifies expressions for SIC vectors, and has other applications to SICs and to Mutually Unbiased Bases. Exact solutions for SICs in dimension 16 are presented for the first time.},
journal = {Quantum Info. Comput.},
month = may,
pages = {404–431},
numpages = {28},
keywords = {representation, clifford group, SIC POVM}
}